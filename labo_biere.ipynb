   "metadata": {
    "colab": {},
    "colab_type": "code",
    "id": "X0E6UcmFOsjq"
   },
   "outputs": [],
   "source": [
    "def average(liste):\n",
    "  return sum(liste)/len(liste)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "colab": {},
    "colab_type": "code",
    "id": "PbK9TUs3OD7o"
   },
   "outputs": [],
   "source": [
    "v_Fe_moy = average(v_Fe)"
